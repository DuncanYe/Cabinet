<div class="container">
  <div class="row">

    <h1 class="text-center">Docs list</h1>

    <h3><%= link_to 'New Doc ', new_doc_path %></h3>
    <br>

    <table class="table table-striped table-bordered" > 
      <tr>
        <th>Ttile</th>
        <th>Content</th>
        <th>Create_at</th>
        <th>Docs_id</th>
        <th>User_id</th>
        <th>#</th>
      </tr>
      <tr>
      <% @docs.each do |d| %>
      <td><%= d.title %></td>
        <td><%= d.content %></td>
        <td><%= time_ago_in_words(d.created_at) %></td>
        <td><%= d.id %></td>
        <td><%= d.user_id %></td>
        <td><%= link_to 'Show', doc_path(d) %>
        <%= link_to 'Edit', edit_doc_path(d) %>
        <%= link_to 'Trash it', doc_path(d), method: :delete, data: { confirm: "Areyou sure? "} %></td>
      </tr>
      <% end %>
    </table>

    
  </div>
</div>
